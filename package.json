{
  "name": "savekaro",
  "version": "1.0.0",
  "description": "SaveKaro - Pakistani Fashion Deal Aggregator",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && source ../.venv/bin/activate && python3 main.py",
    "frontend": "cd frontend && npm start",
    "install-all": "cd backend && pip3 install -r requirements.txt && cd ../frontend && npm install"
  },
  "dependencies": {
    "mongodb": "^7.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
